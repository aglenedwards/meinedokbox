[nix]
channel = "stable-24_05"
packages = ["cairo", "pango", "libuuid", "pkg-config", "graphicsmagick", "ghostscript", "poppler_utils"]

[deployment]
deploymentTarget = "autoscale"
build = ["npm", "run", "build"]
